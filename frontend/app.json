{
  "expo": {
    "name": "DishDive",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B54FB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.josephvbarresi.frontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.josephvbarresi.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "dishdive",
    "extra": {
      "eas": {
        "projectId": "9b417687-35ff-4127-95b9-79152dcf863a"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow DishDive to access your camera",
          "microphonePermission": "Allow DishDive to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "react-native-share",
        {
          "ios": [
            "fb",
            "instagram",
            "twitter",
            "tiktoksharesdk",
            "sms"
          ],
          "android": [
            "com.facebook.katana",
            "com.instagram.android",
            "com.twitter.android",
            "com.zhiliaoapp.musically"
          ],
          "enableBase64ShareAndroid": true
        }
      ],
      [
      "expo-font",
        {
          "fonts": ["node_modules/@expo-google-fonts/montserrat/600SemiBold"]
        }
      ]
    ]
  }
}
