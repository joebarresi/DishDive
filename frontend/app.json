{
  "expo": {
    "name": "DishDive",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.josephvbarresi.frontend",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.josephvbarresi.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9b417687-35ff-4127-95b9-79152dcf863a"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow DishDive to access your camera",
          "microphonePermission": "Allow DishDive to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
